{
    "name": "antaresbr/laravel-socket",
    "description": "A custom socket package for Laravel Framework",
    "homepage": "https://github.com/antaresbr/laravel-socket",
    "keywords": [
        "php",
        "laravel",
        "socket"
    ],
    "authors": [
        {
            "name": "Eduardo Guerra",
            "email": "eduardo.antares@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "antaresbr/foundation": "*",
        "laravel/framework": "^12",
        "nesbot/carbon": "^2.71.0 || ^3.0.0"
    },
    "autoload": {
        "files": [
            "support/helpers.php"
        ],
        "psr-4": {
            "Antares\\Socket\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10"
    },
    "autoload-dev": {
        "psr-4": {
            "Antares\\Socket\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antares\\Socket\\Providers\\SocketServiceProvider"
            ]
        }
    }
}
